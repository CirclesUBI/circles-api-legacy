{
  "name": "user-profile-service",
  "version": "1.0.16",
  "description": "CRUD app for user and organization profiles",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node -r dotenv/config build/index.js",
    "migrate:up": "npm run build:clean && npm run build && DEBUG=knex:query,knex:bindings ./node_modules/.bin/knex migrate:latest",
    "migrate:down": "npm run build:clean && npm run build && DEBUG=knex:query,knex:bindings ./node_modules/.bin/knex migrate:rollback",
    "win_migrate:up": "npm run win_build:clean && npm run build && knex migrate:latest",
    "win_migrate:down": "npm run win_build:clean && npm run build && knex migrate:rollback",
    "migrate:create": "sh scripts/copyMigration.sh",
    "build": "babel src -q -d build/",
    "build:clean": "rm -rf build",
    "win_build:clean": "rimraf build",
    "dev": "babel src -q -d build/ && node -r dotenv/config build/index.js",
    "win_dev": "npm run build && nodemon build/index.js",
    "dev:tunnel": "sh scripts/tunnel.sh && npm run dev",
    "seed": "npm run win_migrate:up && knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CirclesUBI/circles-api.git"
  },
  "author": "ana0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CirclesUBI/circles-api/issues"
  },
  "homepage": "https://github.com/CirclesUBI/circles-api#readme",
  "dependencies": {
    "aws-sdk": "^2.376.0",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.4.0",
    "jwk-to-pem": "^2.0.0",
    "knex": "^0.15.2",
    "objection": "^1.4.0",
    "pg": "^7.6.1",
    "role-acl": "^3.2.0",
    "web3": "^1.0.0-beta.35",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "dotenv": "^6.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.8"
  },
  "nodemonConfig": {
    "events": {
      "restart": "npm run build"
    },
    "exec": "babel-node",
    "presets": "babel-preset-env",
    "watch": [
      "src"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  }
}
